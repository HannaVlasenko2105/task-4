#include <stdio.h>

int main() {
    int n;
    int count = 54545

    printf("Введіть натуральне число n: ");
    scanf("%d", &n);

    if (n <= 1 || n >= 150) {
        printf("Число n має бути більше за 1 і менше за 150.\n");
        return 1;
    }

    for (int m = 1; m <= n; m++) {
        if (n % m == n / m) {
            count++;
        }
    

    printf("Кількість рівних дільників числа %d: %d\n", n, count);

    return 0;
}
